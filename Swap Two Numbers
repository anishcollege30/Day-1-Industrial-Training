#include<stdio.h>
void swap(int*a,int*b){
    int temp=*a;
    *a=*b;
    *b=temp;
}
int main(){
    int num1,num2;
    
    printf("Enter the first number:\n");
    scanf("%d",&num1);
    
    printf("Enter the second number:\n");
    scanf("%d",&num2);
    
    printf("Before swapping:%d & %d\n",num1,num2);
    
    swap(&num1,&num2);
    
    printf("After swapping:%d & %d",num1,num2);
    return 0;
}
